import java.util.List;

/**
 * Created by Andrei on 05/31/2019.
 */
public class Building {
    private List<Floor> floors;

    public List<Floor> getFloors() {
        return floors;
    }

    @Override
    public String toString() {
        return "Building has " + getFloors().size() + " floors.";
    }
}
