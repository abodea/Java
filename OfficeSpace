 */
public class OfficeSpace extends Room {
    private int desks;
    private int officeSpaceNumber;

    public int getDesks() {
        return desks;
    }

    public void setDesks(int desks) {
        this.desks = desks;
    }

    public int getOfficeSpaceNumber() {
        return officeSpaceNumber;
    }

    public void setOfficeSpaceNumber(int officeSpaceNumber) {
        this.officeSpaceNumber = officeSpaceNumber;
    }
    
    @Override
    public String toString() {
        return "Office space " + getOfficeSpaceNumber() + ": " + getDesks() + " desks. \n";
    }
}
